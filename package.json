{
  "name": "recodex-web-app",
  "description": "Single page application for the ReCodEx project.",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Šimon Rozsíval <simon@rozsival.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/recodex/web-app.git"
  },
  "scripts": {
    "clean": "rm public/bundle.js && rm public/vendor.js || true",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --config webpack.config.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=development mocha",
    "test:watch": "npm test -- --watch --reporter nyan",
    "coverage": "babel-node ./node_modules/isparta/bin/isparta cover --root src --report text --report html ./node_modules/mocha/bin/_mocha -- --reporter dot ",
    "coveralls": "babel-node ./node_modules/isparta/bin/isparta cover --root src ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "dev": "cross-env NODE_ENV=development API_BASE=http://localhost:4000 babel-node dev",
    "start": "npm run build && cross-env NODE_ENV=production babel-node src/server",
    "mock-api-server": "json-server --port 4000 --watch tmp/db.json"
  },
  "dependencies": {
    "admin-lte": "^2.3.3",
    "bluebird": "^3.3.5",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.1",
    "cross-env": "^1.0.7",
    "deep-equal": "^1.0.1",
    "ejs": "^2.4.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "js-md5": "^0.4.1",
    "jsonwebtoken": "^6.2.0",
    "jwt-decode": "^2.0.1",
    "react": "^15.0.1",
    "react-avatar": "^0.7.3",
    "react-bootstrap": "^0.29.3",
    "react-dom": "^15.0.1",
    "react-fontawesome": "^1.1.0",
    "react-gravatar": "^2.4.3",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-responsive": "^1.1.3",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "redial": "^0.4.2",
    "redux": "^3.5.1",
    "redux-actions": "^0.9.1",
    "redux-api-middleware": "^1.0.2",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-promise-middleware": "^3.0.0",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.1"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "autoprefixer": "^6.3.6",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^4.5.0",
    "file-loader": "^0.8.5",
    "isomorphic-fetch": "^2.2.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "jsdom": "^9.0.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.10",
    "less-loader": "^2.2.3",
    "marked": "^0.3.5",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "mock-local-storage": "^1.0.2",
    "postcss-loader": "^0.8.2",
    "request": "^2.61.0",
    "sass-loader": "^3.2.0",
    "scroll-behavior": "^0.4.0",
    "serialize-javascript": "^1.2.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-isomorphic-tools": "^2.2.50"
  }
}
